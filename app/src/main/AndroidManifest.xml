<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lute.takeuappointment">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_agenda_splash"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_agenda_splash"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BBMenuClienteActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BALoginActivity" />
        <activity android:name=".CAProfesionalesActivity" />
        <activity android:name=".CBClienteActivity" />
        <activity android:name=".DAConfirmacionClienteActivity" />
        <activity android:name=".DAConfirmacionProfesionalesActivity" />
        <activity android:name=".AASplashActivity"></activity>
    </application>

</manifest>